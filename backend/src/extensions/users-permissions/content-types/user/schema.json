{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "imge": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "fansnum": {
      "type": "biginteger",
      "required": true,
      "default": "0"
    },
    "articlesnum": {
      "required": true,
      "type": "biginteger",
      "default": "0"
    },
    "starednum": {
      "type": "biginteger",
      "required": true,
      "default": "0"
    },
    "subscriptnum": {
      "type": "biginteger",
      "default": "0",
      "required": true
    },
    "storednum": {
      "type": "biginteger",
      "required": true,
      "default": "0"
    },
    "fans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "admin::user"
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    },
    "subscripter": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "admin::user"
    },
    "stored": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    }
  }
}
